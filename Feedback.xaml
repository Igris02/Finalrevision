<Window x:Class="Finalrevision.Feedback"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Finalrevision"
        mc:Ignorable="d"
        Title="Feedback" Height="260" Width="400" WindowStartupLocation="CenterScreen">
    <Grid>
        <TextBox x:Name="FeedbackTextBox" HorizontalAlignment="Center" Margin="0,35,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="348" Height="120"/>
        <Button x:Name="SubmitFeedback" Content="Submit" HorizontalAlignment="Center" Margin="0,166,0,0" VerticalAlignment="Top" Click="SubmitFeedback_Click_1" RenderTransformOrigin="0.5,0.5" Width="214" Height="45">
            
        </Button>
        <Label Content="Your Feedback:" HorizontalAlignment="Left" Margin="20,6,0,0" VerticalAlignment="Top"/>


    </Grid>
</Window>
